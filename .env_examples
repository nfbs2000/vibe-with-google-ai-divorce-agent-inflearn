# Google Cloud Configuration
GOOGLE_CLOUD_PROJECT=your-gcp-project-id
GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account-key.json
BIGQUERY_DATASET=your_bigquery_dataset
BIGQUERY_LOCATION=asia-northeast3
 
# Vertex AI Configuration
VERTEX_AI_REGION=asia-northeast3
VERTEX_AI_ENDPOINT=asia-northeast3-aiplatform.googleapis.com
 
# ADK Model Configuration (Gemini 3.0 Pro Preview, US region only)
ADK_MODEL_NAME=gemini-3-pro-preview
ADK_MODEL_REGION=us-central1
ADK_MODEL_ENDPOINT=us-central1-aiplatform.googleapis.com
 
# API Keys
GOOGLE_API_KEY=your-google-api-key
 
# 공공데이터포털 법제처 API (이혼 판례 검색)
DATA_GO_KR_SERVICE_KEY=your-data-go-kr-service-key

# File Search Store Names
PRECEDENT_FILE_SEARCH_STORE_NAME=fileSearchStores/your-precedent-store-name
 
#ANTHROPIC_API_KEY=your-anthropic-api-key
 


ELEVENLABS_API_KEY=your-elevenlabs-api-key
 
# Database
FIRESTORE_DATABASE=(default)
 
# Storage
GCS_BUCKET_NAME=your-gcs-bucket-name
 
 
# Application
NODE_ENV=development
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-nextauth-secret
 
# ADK Session Memory Configuration
# ENVIRONMENT: development (default), staging, production
ENVIRONMENT=development
 
# MEMORY_SERVICE: inmemory (개발용, 휘발성, 무료) | memorybank (프로덕션용, 영구, LLM 기반)
# 설정하지 않으면 ENVIRONMENT에 따라 자동 선택:
#   - development -> inmemory
#   - staging/production -> memorybank
MEMORY_SERVICE=inmemory
 
# Memory Bank 설정 (MEMORY_SERVICE=memorybank 또는 ENVIRONMENT=production/staging일 때 사용)
GOOGLE_CLOUD_LOCATION=asia-northeast3
 
# Backend API
BACKEND_URL=http://localhost:8004
BACKEND_PORT=8004
FRONTEND_PORT=8005
 
 
# Model Configuration
DEFAULT_MODEL_PROVIDER=gemini
DEFAULT_MODEL_NAME=gemini-1.5-flash
DEFAULT_STORY_PATTERN=sequential
HIGH_QUALITY_STORY_PATTERN=self-reflection
ENABLE_FIRESTORE_PERSISTENCE=true
 

FILE_SEARCH_STORE_NAME=fileSearchStores/your-file-search-store-name
LAW_OC_ID=nfbs2000
 



 
